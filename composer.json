{
    "name": "jaceromri/CurlAxel",
    "description": "PHP based download accelerator",
    "license": "MIT",
    "keywords": ["boilerplate", "http"],
    "authors": [
        {
            "name": "Jacer Omri",
            "email": "jaceromri@gmail.com"
        }
    ],
    "require": {
        "php": "^5.5 || ^7.0",
        "php-curl-class/php-curl-class": "^8.1"
    },
    "autoload": {
        "psr-4": {
            "CurlAxel\\": "src/"
        },
        "classmap": [
            "src/"
        ]
    },
    "scripts": {
        "test": "",
        "test-ci": ""
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "require-dev": {
        "phpunit/phpunit": "^7",
        "squizlabs/php_codesniffer": "3.*"
    }
}
